{
  "name": "dive",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "packageManager": "pnpm@7.12.1",
  "scripts": {
    "dev": "pnpm codegen && vite",
    "build": "pnpm codegen && tsc && vite build",
    "preview": "vite preview",
    "lint": "pnpm codegen && eslint app --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint app --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck": "pnpm codegen && tsc",
    "stylelint": "stylelint --cache --ignore-path .gitignore **/*.css",
    "stylelint:fix": "stylelint --cache --fix --ignore-path .gitignore **/*.css",
    "test": "vitest",
    "codegen": "graphql-codegen --require dotenv/config --config codegen.yml dotenv_config_path=./.env.local",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/core": "^7.21.3",
    "@emotion/react": "^11.10.6",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@mantine/core": "^5.10.5",
    "@mantine/form": "^5.10.5",
    "@mantine/hooks": "^5.10.5",
    "@sentry/react": "^7.45.0",
    "@sentry/tracing": "^7.45.0",
    "@togglecorp/fujs": "^2.0.0",
    "@urql/exchange-multipart-fetch": "^1.0.1",
    "graphql": "^16.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "4.8.0",
    "react-router-dom": "^6.9.0",
    "urql": "^3.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/client-preset": "2.1.1",
    "@graphql-codegen/introspection": "3.0.1",
    "@graphql-codegen/schema-ast": "3.0.0",
    "@graphql-eslint/eslint-plugin": "^3.17.0",
    "@types/node": "^18.15.10",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "browserslist": "^4.21.5",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "pnpm": "^7.30.3",
    "postcss": "^8.4.21",
    "postcss-nested": "^6.0.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^8.1.0",
    "stylelint": "^14.16.1",
    "stylelint-config-concentric-order": "^5.1.0",
    "stylelint-config-recommended": "^9.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "typescript-plugin-css-modules": "^4.2.3",
    "vite": "^4.2.3",
    "vite-plugin-checker": "^0.5.6",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^0.14.6",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.7",
    "vitest": "^0.29.7"
  }
}
